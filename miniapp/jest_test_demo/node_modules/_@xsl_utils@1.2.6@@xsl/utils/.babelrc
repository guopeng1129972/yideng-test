{
  "presets": [
    "env",
    "stage-0"
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-runtime",
        "add-module-exports"
      ]
    },
    "test": {
      "plugins": [
        "transform-runtime",
        "add-module-exports",
        "babel-plugin-espower"
      ]
    },
    "production": {
      "plugins": [
        "transform-runtime",
        "add-module-exports"
      ]
    }
  }
}
