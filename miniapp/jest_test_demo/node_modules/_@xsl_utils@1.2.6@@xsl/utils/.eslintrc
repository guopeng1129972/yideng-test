{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "airbnb-base"
  ],
  "plugins": [
    "babel",
    "import"
  ],
  "settings": {
    "import/ignore": [
      "node_modules",
      "\\.json$"
    ],
    "import/parser": "babel-eslint",
    "import/resolve": {
      "extensions": [
        ".js",
        ".json"
      ]
    }
  },
  "rules": {
    "import/named": 0,
    "semi": [
      2,
      "never"
    ],
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-alert": 0,
    "no-console": 0,
    "no-undef": 1,
    "id-length": 0,
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ],
    "new-cap": [
      2,
      {
        "capIsNewExceptions": [
          "express.Router"
        ]
      }
    ],
    "max-len": [
      2,
      120,
      2,
      {
        "ignoreUrls": true,
        "ignoreComments": false
      }
    ],
    "no-mixed-operators": [
      2,
      {
        "allowSamePrecedence": true
      }
    ],
    "generator-star-spacing": 0,
    "newline-per-chained-call": 0,
    "no-plusplus": [
      2,
      {
        "allowForLoopAfterthoughts": true
      }
    ],
    "global-require": 0,
    "no-unused-expressions": [
      2,
      {
        "allowShortCircuit": true
      }
    ]
  },
  "globals": {
    "test": true,
    "expect": true,
    "setTimeout": true
  }
}
